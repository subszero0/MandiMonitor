  "test_scenarios": {
    "field_precedence": {
      "description": "Test data to verify field precedence logic",
      "scenarios": [
        {
          "name": "technical_info_wins",
          "description": "Technical info should override features and title",
          "test_data": {
            "asin": "TEST_PRECEDENCE_1",
            "title": "Monitor 32 inch 60Hz",
            "features": ["27-inch display", "120Hz refresh rate"],
            "technical_details": {
              "screen_size": "24 inches", 
              "refresh_rate": "144 Hz"
            }
          },
          "expected": {
            "size": {"value": "24", "source": "technical_info"},
            "refresh_rate": {"value": "144", "source": "technical_info"}
          }
        },
        {
          "name": "features_win_over_title", 
          "description": "Features should override title when technical_info missing",
          "test_data": {
            "asin": "TEST_PRECEDENCE_2",
            "title": "Samsung 32 inch 60Hz Monitor",
            "features": ["27-inch curved display", "144Hz refresh rate"],
            "technical_details": {}
          },
          "expected": {
            "size": {"value": "27", "source": "features"},
            "refresh_rate": {"value": "144", "source": "features"},
            "curvature": {"value": "curved", "source": "features"}
          }
        }
      ]
    },
